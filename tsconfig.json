{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "@/*": ["src/*"],
      "@domain/*": ["src/domain/*"],
      "@data/*": ["src/data/*"],
      "@presentation/*": ["src/presentation/*"],
      "@main/*": ["src/main/*"],
      "@database/*": ["src/database/*"],
      "@validation/*": ["src/validation/*"],
      "@assets/*": ["assets/*"],
      "@utils/*": ["src/utils/*"]
    }
  }
}
